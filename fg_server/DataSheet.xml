
<!-- 静态数据表 -->

<DataSheet>
    <Sheet excelName="测试表" name="测试" toName="Test">
        <Index fields="ID" type="PK"/>
        <Index fields="Type, Name" type="UNIQUE"/>
        <Field name="ID" toName="ID" dataType="int"/>
        <Field name="编号" toName="Type" dataType="int"/>
        <Field name="名称" toName="Name" dataType="string"/>
    </Sheet>
    <Sheet excelName="测试表" name="测试2" toName="Test2" useTip="C">
        <Index fields="ID2, Name2" type="UNIQUE"/>
        <Index fields="ID2, Type2" type="UNIQUE"/>
        <Field name="ID" toName="ID2" dataType="int"/>
        <Field name="编号" toName="Type2" dataType="int"/>
        <Field name="名称" toName="Name2" dataType="string"/>
    </Sheet>
    <Sheet excelName="人物表" name="人物" toName="Player">
        <Index fields="Name, Level" type="KEY"/>
        <Index fields="ID, Name" type="UNIQUE"/>
        <Index fields="ID" type="PK"/>
        <Field name="ID" toName="ID" dataType="int"/>
        <Field name="名称" toName="Name" dataType="string"/>
        <Field name="品质" toName="Quality" dataType="int"/>
        <Field name="等级" toName="Level" dataType="int"/>
    </Sheet>
    <Sheet excelName="怪物表" name="怪物表" toName="Monster">
        <Index fields="MonsterQuality,  MonsterLevel" type="KEY"/>
        <Index fields="ID" type="PK"/>
        <Field name="ID" toName="ID" dataType="int"/>
        <Field name="怪物表ID" toName="MonsterID" dataType="int"/>
        <Field name="怪物表名称" toName="MonsterName" dataType="string"/>
        <Field name="怪物表品质" toName="MonsterQuality" dataType="int"/>
        <Field name="怪物表等级" toName="MonsterLevel" dataType="int"/>
    </Sheet>
    <Sheet excelName="NPC表" name="NPC表" toName="NPC">
        <Index fields="NPCPosX,  NPCPosY, npcPosZ" type="KEY"/>
        <Index fields="ID" type="PK"/>
        <Index fields="Name" type="KEY"/>
        <Field name="ID" toName="ID" dataType="int"/>
        <Field name="NPC表ID" toName="NPCID" dataType="int"/>
        <Field name="NPC名称" toName="NPCName" dataType="string"/>
        <Field name="NPC品质" toName="NPCQuality" dataType="int"/>
        <Field name="NPC位置x" toName="NPCPosX" dataType="int"/>
        <Field name="NPC位置y" toName="NPCPosY" dataType="int"/>
        <Field name="NPC位置z" toName="NPCPosZ" dataType="int"/>
        <Field name="NPC等级" toName="NPCLevel" dataType="int"/>
    </Sheet>
</DataSheet>
